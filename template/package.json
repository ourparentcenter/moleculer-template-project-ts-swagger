{
  "name": "{{projectName}}",
  "version": "1.0.0",
  "description": "{{projectdescription}}",
  "scripts": {
    "build": "tsc --build tsconfig.json"{{#if_eq watcher "TSNODE"}},
    "dev": "cross-env NODE_ENV=development dotenv-flow -p ./env -- ts-node -r tsconfig-paths/register ./node_modules/moleculer/bin/moleculer-runner.js --hot --repl --config moleculer.config.ts"{{/if_eq}}{{#if_eq watcher "TSNODEDEV"}},
    "dev": "cross-env NODE_ENV=development dotenv-flow -p ./env -- ts-node-dev -r tsconfig-paths/register ./node_modules/moleculer/bin/moleculer-runner.js --hot --repl --config moleculer.config.ts"{{/if_eq}},
    "start": "ts-node -r tsconfig-paths/register ./node_modules/moleculer/bin/moleculer-runner.js --config moleculer.config.ts",
    "cli": "moleculer connect {{transporter}}",
    "ci": "jest --watchAll",
    "test": "jest --coverage"{{#lint}},
    "lint": "eslint --ext .js,.ts ."{{/lint}}{{#docker}},
    "dc:up": "docker-compose up --build -d",
    "dc:logs": "docker-compose logs -f",
    "dc:down": "docker-compose down"{{/docker}}
  },
  "keywords": [
    "microservices",
    "moleculer"
  ],
  "repository": {
    "type": "git",
    "url": "git+"
  },
  "homepage": "#readme",
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
		"@types/bcryptjs": "^2.4.2",
		"@types/jest": "^29.5.1",
    "@types/js-yaml": "^4.0.5",
		"@types/mkdirp": "^1.0.2",
		"@types/node": "^^18.16.1",
    "@types/randomstring": "^1.1.8",
    "@types/rimraf": "^3.0.2",
		"@types/supertest": "^2.0.12",
    "@types/swagger-jsdoc": "^6.0.1",
    {{#apiGW}}
    "@types/cookie-parser": "^1.4.3",
    {{/apiGW}}
    "@types/dotenv-flow": "^3.2.0",
		"@types/jsonwebtoken": "^9.0.1",
		"@types/lodash": "^4.14.194",
    {{#swagger}}
		"@types/swagger-ui-dist": "^3.30.1",
    {{/swagger}}
    {{#swaggerstats}}
    "@types/swagger-stats": "^0.95.8",
    {{/swaggerstats}}
    "bcrypt": "^5.1.0",
    "chokidar": "^3.5.3",
    {{#lint}}
    "eslint": "^8.39.0",
    "eslint-config-prettier": "^8.8.0",
		"eslint-plugin-import": "^2.27.5",
		"eslint-plugin-prettier": "^4.2.1",
		"@typescript-eslint/eslint-plugin": "^5.59.1",
		"@typescript-eslint/parser": "^5.59.1",
		"eslint-plugin-prefer-arrow": "^1.2.3",
    {{/lint}}
    "jest": "^29.5.0",
    "jest-chain": "^1.1.6",
		"jest-circus": "^29.5.0",
    "jest-cli": "^29.5.0",
    "jest-junit": "^16.0.0",
    "jest-extended": "^3.2.4",
    "majestic": "^1.8.1",
    "moleculer-repl": "^0.7.3",
    {{#apiGW}}
    "open": "^8.4.2",
    {{/apiGW}}
    "prettier": "^2.8.8",
    "supertest": "^6.3.3",
		"tsconfig-paths": "^4.2.0",
		"ts-jest": "^29.1.0",
    {{#if_eq watcher "TSNODE"}}
    "ts-node": "^10.9.1"
    {{/if_eq}}
    {{#if_eq watcher "TSNODEDEV"}}
    "ts-node-dev": "^1.1.8"
    {{/if_eq}}
  },
  "dependencies": {
    {{#if_eq transporter "NATS"}}
    "nats": "^2.7.1",
    {{/if_eq}}
    {{#if_eq transporter "MQTT"}}
    "mqtt": "^4.2.8",
    {{/if_eq}}
    {{#if_eq transporter "AMQP"}}
    "amqplib": "^0.8.0",
    {{/if_eq}}
    {{#if_eq transporter "AMQP10"}}
    "rhea-promise": "^2.1.0",
    {{/if_eq}}
    {{#if_eq transporter "STAN"}}
    "node-nats-streaming": "^0.3.2",
    {{/if_eq}}
    {{#if_eq transporter "Kafka"}}
    "kafka-node": "^5.0.0",
    {{/if_eq}}
    {{#redis}}
    "ioredis": "^4.27.7",
    "redlock": "^4.2.0",
    {{/redis}}
    "js-yaml": "^4.1.0",
    "replace-in-file": "^6.3.5",
    "swagger-jsdoc": "^6.2.8",
    {{#swagger}}
		"swagger-ui-dist": "^4.18.3",
    {{/swagger}}
    {{#swaggereditor}}
    "swagger-editor-dist": "^4.9.1",
    {{/swaggereditor}}
    {{#swaggerstats}}
    "openapi-types": "^12.1.0",
		"swagger-stats": "^0.99.2",
		"prom-client": "^14.2.0",
    {{/swaggerstats}}
    "bcryptjs": "^2.4.3",
    {{#apiGW}}
    "cookie-parser": "^1.4.6",
    {{/apiGW}}
    "core-js": "^3.30.1",
		"cross-env": "^7.0.3",
		"csv-parse": "^5.3.8",
		"dotenv": "^16.0.3",
		"dotenv-flow": "^3.2.0",
		"dotenv-flow-cli": "^1.0.0",
    {{#apiGW}}
    "helmet": "^6.1.5",
    {{/apiGW}}
    "json2typescript": "^1.5.1",
    "jose": "^4.14.2",
		"jsonwebtoken": "^9.0.0",
		"lodash": "^4.17.21",
    "log4js": "^6.9.1",
		"moleculer": "^0.14.29",
		"moleculer-db": "^0.8.23",
    {{#dbService}}
    "moleculer-db-adapter-mongo": "^0.4.17",
		"moleculer-db-adapter-mongoose": "^0.9.2",
    {{/dbService}}
    "@ourparentcenter/moleculer-decorators-extended": "^2.0.0",
    {{#apiGW}}
    "moleculer-web": "^0.10.5",
    {{/apiGW}}
    "mongodb": "^5.3.0",
		"mongoose": "^7.0.5",
    "randomstring": "^1.2.3",
		"reflect-metadata": "^0.1.13",
    {{#if_eq httptransport "SOCKET"}}
    "socket.io": "^4.1.3",
    {{/if_eq}}
    "ts-results": "^3.3.0",
    "typescript": "^5.0.4"
},
  "engines": {
    "node": ">= 16.x.x"
  },
  "jest": {
		"coverageDirectory": "<rootDir>/coverage",
		"testEnvironment": "node",
		"collectCoverage": false,
		"coveragePathIgnorePatterns": [
			"<rootDir>/moleculer.config.ts",
			"<rootDir>/middlewares/OpenInBrowser.ts"
		],
		"reporters": [
			"default",
			"jest-junit"
		],
		"setupFiles": [
			"./jest.setup.ts"
		],
		"setupFilesAfterEnv": [
			"jest-chain",
			"jest-extended/all"
		],
		"moduleNameMapper": {
			"^@MicroServices/(.*)$": "<rootDir>/services/$1",
			"^@MicroServices(.*)$": "<rootDir>/services/$1",
			"^@Entities/(.*)$": "<rootDir>/entities/$1",
			"^@Entities(.*)$": "<rootDir>/entities/$1",
			"^@Factories/(.*)$": "<rootDir>/factories/$1",
			"^@Factories(.*)$": "<rootDir>/factories/$1",
			"^@Middlewares/(.*)$": "<rootDir>/middlewares/$1",
			"^@Middlewares(.*)$": "<rootDir>/middlewares/$1",
			"^@Mixins/(.*)$": "<rootDir>/mixins/$1",
			"^@Mixins(.*)$": "<rootDir>/mixins/$1",
			"^@ServiceHelpers/(.*)$": "<rootDir>/helpers/$1",
			"^@ServiceHelpers(.*)$": "<rootDir>/helpers/$1"
		},
		"moduleFileExtensions": [
			"ts",
			"tsx",
			"js"
		],
		"transform": {
			"^.+\\.(ts|tsx)$": [
				"ts-jest",
				{
					"tsconfig": "tsconfig.json",
					"diagnostics": true
				}
			]
		},
		"testMatch": [
			"**/*.spec.(ts|js)"
		]
	}
}
