{
  "name": "{{projectName}}",
  "version": "1.0.0",
  "description": "My Moleculer-based microservices project",
  "scripts": {
    "build": "tsc --build tsconfig.json"{{#if_eq watcher "TSNODE"}},
    "dev": "cross-env NODE_ENV=development dotenv-flow -p ./env -- ts-node -r tsconfig-paths/register ./node_modules/moleculer/bin/moleculer-runner.js --hot --repl --config moleculer.config.ts"{{/if_eq}}{{#if_eq watcher "TSNODEDEV"}},
    "dev": "cross-env NODE_ENV=development dotenv-flow -p ./env -- ts-node-dev -r tsconfig-paths/register ./node_modules/moleculer/bin/moleculer-runner.js --hot --repl --config moleculer.config.ts"{{/if_eq}},
    "start": "moleculer-runner",
    "cli": "moleculer connect {{transporter}}",
    "ci": "jest --watch",
    "test": "jest --coverage"{{#lint}},
    "lint": "eslint --ext .js,.ts ."{{/lint}}{{#docker}},
    "dc:up": "docker-compose up --build -d",
    "dc:logs": "docker-compose logs -f",
    "dc:down": "docker-compose down"{{/docker}}
  },
  "keywords": [
    "microservices",
    "moleculer"
  ],
  "author": "",
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/cookie-parser": "^1.4.2",
    "@types/dotenv-flow": "^3.1.0",
    "@types/helmet": "^4.0.0",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/lodash": "^4.14.168",
    "@types/mongoose": "^5.10.3",
    "bcrypt": "^5.0.1",
    "chokidar": "^3.5.1",
    {{#lint}}
    "eslint": "^7.20.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-config-prettier": "^7.2.0",
		"eslint-plugin-prettier": "^3.3.1",
    "@typescript-eslint/eslint-plugin": "^4.15.2",
    "@typescript-eslint/parser": "^4.15.2",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    {{/lint}}
    "jest": "^26.6.3",
    "jest-cli": "^26.6.3",
    "moleculer-repl": "^0.6.4",
    "prettier": "^2.2.1",
    "tsconfig-paths": "^3.9.0",
    "ts-jest": "^26.5.1",
    {{#if_eq watcher "TSNODE"}}
    "ts-node": "^9.1.1"
    {{/if_eq}}
    {{#if_eq watcher "TSNODEDEV"}}
    "ts-node-dev": "^1.1.1"
    {{/if_eq}}
  },
  "dependencies": {
    "@types/jest": "^26.0.20",
    "@types/mkdirp": "^1.0.1",
    "@types/node": "^14.14.31",
    {{#if_eq transporter "NATS"}}
    "nats": "^1.4.12",
    {{/if_eq}}
    {{#if_eq transporter "MQTT"}}
    "mqtt": "^4.2.6",
    {{/if_eq}}
    {{#if_eq transporter "AMQP"}}
    "amqplib": "^0.6.0",
    {{/if_eq}}
    {{#if_eq transporter "AMQP10"}}
    "rhea-promise": "^1.1.0",
    {{/if_eq}}
    {{#if_eq transporter "STAN"}}
    "node-nats-streaming": "^0.3.2",
    {{/if_eq}}
    {{#if_eq transporter "Kafka"}}
    "kafka-node": "^5.0.0",
    {{/if_eq}}
    {{#redis}}
    "ioredis": "^4.22.0",
    {{/redis}}
    {{#swagger}}
    "@types/swagger-jsdoc": "^6.0.0",
		"@types/swagger-ui-dist": "^3.30.0",
    "swagger-jsdoc": "^6.0.6",
		"swagger-ui-dist": "^3.43.0",
    {{/swagger}}
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.5",
    "cross-env": "^7.0.3",
    "csv-parse": "^4.15.3",
    "dotenv": "^8.2.0",
    "dotenv-flow": "^3.2.0",
    "dotenv-flow-cli": "^1.0.0",
    "helmet": "^4.4.1",
    "json2typescript": "^1.4.1",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "moleculer": "^0.14.12",
    "moleculer-db": "^0.8.12",
    "moleculer-db-adapter-mongo": "^0.4.11",
    "moleculer-db-adapter-mongoose": "^0.8.9",
    "moleculer-decorators-extended": "file:../moleculer-decorators-extended",
    "moleculer-web": "^0.9.1",
    "mongodb": "^3.6.4",
    "mongoose": "^5.11.19",
    "reflect-metadata": "^0.1.13",
    "ts-results": "^3.1.1",
    "typescript": "^4.1.5"
},
  "engines": {
    "node": ">= 10.x.x"
  },
  "jest": {
    "coverageDirectory": "<rootDir>/coverage",
    "testEnvironment": "node",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "testMatch": [
      "**/*.spec.(ts|js)"
    ],
    "globals": {
      "ts-jest": {
        "tsconfig": "tsconfig.json"
      }
    }
  }
}
